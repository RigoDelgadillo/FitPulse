@model PIAProWeb.Models.dbModels.GruposMusculare

@{
    ViewData["Title"] = "Create";
}
<link rel="stylesheet" href="css/Crear.css">
<h1>Create</h1>

<h4>GruposMusculare</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Musculo" class="control-label"></label>
                <input asp-for="Musculo" class="form-control" />
                <span asp-validation-for="Musculo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="btn btn-danger" asp-action="Index">Regresar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
